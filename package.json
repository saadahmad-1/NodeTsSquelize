    "@types/supertest": "^2.0.16",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "db-migrate": "^0.11.14",
    "eslint": "^8.55.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.1.0",
    "nodemon": "^3.0.2",
